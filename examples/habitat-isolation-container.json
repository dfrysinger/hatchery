{
  "name": "CodeRunner",
  "domain": "mybot.example.com",
  "destructMinutes": 0,
  "bgColor": "2D3748",
  "platform": "discord",
  "isolation": "container",
  "sharedPaths": ["/clawd/shared/code"],
  "discord": {
    "serverId": "YOUR_DISCORD_SERVER_ID",
    "ownerId": "YOUR_DISCORD_USER_ID"
  },
  "council": {
    "groupName": "The Council",
    "judge": "Opus"
  },
  "globalIdentity": "You are a helpful assistant running on a cloud VM.",
  "globalBoot": "",
  "globalBootstrap": "",
  "globalSoul": "",
  "globalAgents": "",
  "globalUser": "- Name: Your Name",
  "agents": [
    {
      "agent": "Orchestrator",
      "discordBotToken": "YOUR_ORCHESTRATOR_DISCORD_BOT_TOKEN",
      "isolation": "session",
      "isolationGroup": "trusted"
    },
    {
      "agent": "CodeExecutor",
      "discordBotToken": "YOUR_EXECUTOR_DISCORD_BOT_TOKEN",
      "isolationGroup": "sandbox",
      "network": "none",
      "capabilities": ["exec"],
      "resources": {"memory": "512Mi"}
    },
    {
      "agent": "Researcher",
      "discordBotToken": "YOUR_RESEARCHER_DISCORD_BOT_TOKEN",
      "isolationGroup": "sandbox",
      "network": "internal",
      "capabilities": ["web_search", "web_fetch"]
    }
  ]
}
